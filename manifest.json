{
  "manifest_version": 3,
  "name": "Simple Grayscale",
  "version": "1.0",
  "description": "Converts the webpage grayscale  expect for images",
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  }
}
